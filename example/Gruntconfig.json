{
  "srcPaths": {
    "make": "src/project.make",
    "drupal": "src"
  },
  "buildPaths": {
    "build": "build",
    "html": "build/html",
    "package": "build/packages",
    "reports": "build/reports",
    "temp": "build/temp"
  },
  "domain": "project.vm",
  "siteUrls": {
    "default": "http://<%= config.domain %>",
    "subsite.project.vm": "https://subsite.<%= config.domain %>"
  },
  "packages": {
    "srcFiles": ["!sites/*/files/**", "!xmlrpc.php", "!modules/php/*"],
    "projFiles": ["README*", "bin/**"]
  },
  "phpcs": {
    "path": "bin/phpcs",
    "standard": "vendor/drupal/coder/coder_sniffer/Drupal"
  },
  "phpmd": {
    "path": "bin/phpmd"
  },
  "drush": {
    "make": {
      "args": []
    }
  },
  "compassConfig": {}
}
